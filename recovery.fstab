# mount point		fstype	device						[device2]
/external_sd		vfat	/dev/block/mmcblk0p1		/dev/block/mmcblk0
/system				ext4	/dev/block/mtdblock9
/cache				ext4	/dev/block/mtdblock6
/data				ext4	/dev/block/mtdblock11		length=-16384

